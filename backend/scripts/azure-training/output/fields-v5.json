  "labelingGuidelines": {
    "ogun_dagilimi": [
      "Tablonun başlığında 'öğün', 'yemek dağılımı', 'kişi sayısı' gibi ifadeler arayın",
      "Kahvaltı/Öğle/Akşam satırlarını ve karşılık gelen sayıları etiketleyin",
      "Normal/Diyet/Refakatçi gibi kategoriler varsa bunları da dahil edin",
      "Toplam satırını mutlaka işaretleyin"
    ],
    "personel_tablosu": [
      "Tablonun başlığında 'personel', 'işçi', 'kadro' gibi ifadeler arayın",
      "Her pozisyonu (Aşçı, Garson, Bulaşıkçı vb.) ve sayısını etiketleyin",
      "Nitelik gereksinimleri (Ustalık belgesi, Hijyen sertifikası) varsa bunları da dahil edin",
      "Toplam personel sayısını mutlaka işaretleyin"
    ],
    "haftalik_menu": [
      "Gün isimleri (Pazartesi-Pazar) ile tanımlayın",
      "Kaçıncı hafta olduğunu not edin (1. Hafta, 2. Hafta vb.)",
      "Çorba, Ana Yemek, Pilav, Salata, Tatlı sütunlarını işaretleyin"
    ],
    "gramaj_tablosu": [
      "Yemek adı ve karşılık gelen gram değerlerini etiketleyin",
      "Çiğ ve pişmiş gramaj ayrımı varsa belirtin",
      "Porsiyon miktarlarını işaretleyin"
    ]

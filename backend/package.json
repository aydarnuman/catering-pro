{
  "name": "catering-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "migrate": "echo '⚠️  Supabase CLI kullanın: supabase db push' && exit 1",
    "migrate:status": "cd .. && supabase migration list",
    "migrate:new": "cd .. && supabase migration new",
    "migrate:push": "cd .. && supabase db push",
    "migrate:reset": "cd .. && supabase db reset",
    "db:diff": "cd .. && supabase db diff",
    "db:types": "cd .. && supabase gen types typescript --local > frontend/src/types/database.ts",
    "scraper": "node src/scraper/main.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:bordro": "vitest run --testPathPattern=bordro",
    "lint": "npx @biomejs/biome check src/",
    "lint:fix": "npx @biomejs/biome check --write src/",
    "audit:deps": "npm audit --json"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@azure/ai-form-recognizer": "^5.1.0",
    "@azure/storage-blob": "^12.30.0",
    "@supabase/supabase-js": "^2.89.0",
    "adm-zip": "^0.5.16",
    "ajv": "^8.17.1",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "file-type": "^21.2.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.12",
    "pako": "^1.0.11",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "puppeteer": "^22.15.0",
    "resend": "^6.7.0",
    "sharp": "^0.34.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tesseract.js": "^7.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "supertest": "^6.3.3",
    "vitest": "^4.0.18"
  }
}
